<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:ContohXamarin"
             x:Class="ContohXamarin.MainPage">
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Daftar Dokter" Order="Secondary" Clicked="DaftarDokterItem_Clicked" />
        <ToolbarItem Text="Calculator" Order="Secondary" Clicked="CalculatorItem_Clicked" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <Label Text="Firstname :" />
            <Entry x:Name="txtFirstname" Placeholder="insert firstname" />
            <Label Text="Lastname :"/>
            <Entry x:Name="txtLastname" Placeholder="insert lastname"/>
            <StackLayout Orientation="Horizontal">
                <Button x:Name="btnSubmit" Text="Submit" />
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
   
</ContentPage>